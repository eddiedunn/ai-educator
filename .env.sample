# Sample Environment Configuration
# Copy this file to .env and modify as needed

# Application environment variables
REACT_APP_CHAIN_ID=31337
REACT_APP_RPC_URL=http://127.0.0.1:8545
REACT_APP_ENABLE_DEBUG=false  # Set to 'true' to enable debug messages

# IPFS Configuration
REACT_APP_USE_IPFS=false      # Set to 'true' to use real IPFS in production
REACT_APP_IPFS_GATEWAY=https://ipfs.io/ipfs/
REACT_APP_IPFS_HOST=ipfs.infura.io
REACT_APP_IPFS_PORT=5001
REACT_APP_IPFS_PROTOCOL=https
REACT_APP_IPFS_API_PATH=/api/v0
REACT_APP_IPFS_PROJECT_ID=your-infura-ipfs-project-id
REACT_APP_IPFS_PROJECT_SECRET=your-infura-ipfs-project-secret

# OpenAI API Key for question generation
OPENAI_API_KEY=your_openai_api_key_here

# Chainlink Functions Configuration
CHAINLINK_SUBSCRIPTION_ID=your_subscription_id_here

# Contract Addresses
# These will be populated automatically during deployment
# but can be manually set for connecting to existing contracts
PUZZLEPOINTS_ADDRESS=
QUESTIONMANAGER_ADDRESS=
CHAINLINKANSWERVERIFIER_ADDRESS=

# Network Information
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your-api-key

# Account Private Key (without 0x prefix)
# WARNING: Never commit the actual private key to git!
PRIVATE_KEY=your-private-key-here-without-0x-prefix

# Optional: Etherscan API Key for contract verification
ETHERSCAN_API_KEY=your-etherscan-api-key

# Optional: Set to true to enable gas reporting
REPORT_GAS=false 

# OpenAI API Key for question generation
REACT_APP_OPENAI_API_KEY=your-openai-api-key 